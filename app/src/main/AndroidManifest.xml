<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jwilliams.machinistmate.app">


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/ic_action_logo"
        android:title="Machinist Mate"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jwilliams.machinistmate.app.ItemListActivity"
            android:label="Machinist Mate"
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.ItemDetailActivity"
            android:label=""
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.SpeedsDetailActivity"
            android:label="Speeds"
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.FeedsDetailActivity"
            android:label="Feeds"
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.ConversionDetailActivity"
            android:label="Conversions"
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.GeometryPagerActivity"
            android:label="Geometry"
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
            android:name="com.jwilliams.machinistmate.app.ReferencePagerActivity"
            android:label="Reference"
            android:parentActivityName="com.jwilliams.machinistmate.app.ItemListActivity"
            android:screenOrientation="nosensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jwilliams.machinistmate.app.ItemListActivity" />
        </activity>
        <activity
        android:name=".FractionMetricActivity"
        android:label="Fraction Metric Chart"
        android:parentActivityName="com.jwilliams.machinistmate.app.ReferencePagerActivity"
        android:screenOrientation="nosensor">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.jwilliams.machinistmate.app.ReferenceChoiceActivity" />
    </activity>
    </application>

</manifest>
