    <style name="ActionBar" parent="android:style/Widget.Holo.ActionBar">
        <item name="android:titleTextStyle">@style/TitleTextStyle</item>
        <item name="android:background">@drawable/actionbar_background</item>
    </style>

    <style name="AnswerStyle" parent="BaseView">
        <item name="android:textSize">45sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="AppTheme" parent="android:Theme.Holo">

        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/ActionBar</item>
        <item name="android:logo">@drawable/ic_action_logo</item>
        <item name="android:windowBackground">@drawable/fragment_background</item>
    </style>

    <style name="Base">
        <item name="android:background">#111111</item>
    </style>

    <style name="BaseView">
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textColor">#999999</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:background">@drawable/edit_text_background</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textColor">#999999</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:gravity">bottom|right</item>
        <item name="android:hint">Enter value</item>
    </style>

    <style name="HeaderStyle" parent="BaseView">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#111111</item>
    </style>

    <style name="TextViewStyle" parent="BaseView">
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">bottom</item>
    </style>

    <style name="TitleTextStyle" parent="android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#999999</item>
        <item name="android:background">@drawable/actionbar_background</item>
    </style>

    <style name="TypeStyle" parent="BaseView">
        <item name="android:textSize">32sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="buttons">
        <item name="android:background">@drawable/button_background</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
    </style>

    <style name="radioButtons" parent="BaseView"></style>

